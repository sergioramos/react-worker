{
  "env": {
    "node": true
  },
  "ecmaFeatures": {},
  "extends": "eslint:recommended",
  "rules": {
    "comma-dangle": [
      2,
      "never"
    ],
    "no-unexpected-multiline": 2,
    "valid-jsdoc": [
      2, {
        "prefer": {
          "return": "returns"
        },
        "requireReturn": false,
        "requireParamDescription": false,
        "requireReturnDescription": false
      }
    ],
    "block-scoped-var": 2,
    "complexity": [
      2,
      11
    ],
    "consistent-return": 2,
    "curly": [
      2,
      "all"
    ],
    "default-case": 2,
    "dot-location": [
      2,
      "property"
    ],
    "dot-notation": [
      2, {
        "allowKeywords": true
      }
    ],
    "eqeqeq": 2,
    "guard-for-in": 2,
    "no-eval": 2,
    "no-eq-null": 2,
    "no-empty-pattern": 2,
    "no-empty-label": 2,
    "no-else-return": 2,
    "no-div-regex": 2,
    "no-caller": 2,
    "no-alert": 2,
    "no-extend-native": 2,
    "no-magic-numbers": 2,
    "no-loop-func": 2,
    "no-lone-blocks": 2,
    "no-labels": 2,
    "no-iterator": 2,
    "no-invalid-this": 2,
    "no-implied-eval": 2,
    "no-implicit-coercion": 2,
    "no-floating-decimal": 2,
    "no-extra-bind": 2,
    "wrap-iife": [
      2,
      "inside"
    ],
    "radix": 2,
    "no-with": 2,
    "no-void": 2,
    "no-useless-concat": 2,
    "no-useless-call": 2,
    "no-unused-expressions": 2,
    "no-throw-literal": 2,
    "no-sequences": 2,
    "no-self-compare": 2,
    "no-script-url": 2,
    "no-return-assign": 2,
    "no-proto": 2,
    "no-param-reassign": 2,
    "no-octal-escape": 2,
    "no-new": 2,
    "no-new-wrappers": 2,
    "no-new-func": 2,
    "no-native-reassign": 2,
    "no-multi-str": 2,
    "no-multi-spaces": 2,
    "no-use-before-define": 2,
    "no-unused-vars": 2,
    "no-undefined": 2,
    "no-shadow-restricted-names": 2,
    "no-label-var": 2,
    "no-catch-shadow": 2,
    "no-sync": 2,
    "no-path-concat": 2,
    "no-new-require": 0,
    "no-mixed-requires": 2,
    "handle-callback-err": 2,
    "global-require": 2,
    "callback-return": 2,
    "wrap-regex": 2,
    "spaced-comment": [
      2,
      "always", {
        "line": {
          "exceptions": [
            "-"
          ]
        }
      }
    ],
    "space-unary-ops": 2,
    "space-return-throw-case": 2,
    "space-infix-ops": 2,
    "space-in-parens": [
      0,
      "never"
    ],
    "space-before-keywords": [
      2,
      "always"
    ],
    "space-before-function-paren": 0,
    "space-before-blocks": [
      2,
      "always"
    ],
    "space-after-keywords": [
      2,
      "always"
    ],
    "sort-vars": 0,
    "semi": [
      2,
      "always"
    ],
    "semi-spacing": 2,
    "require-jsdoc": 0,
    "quotes": [
      2,
      "single"
    ],
    "quote-props": [
      2,
      "as-needed"
    ],
    "padded-blocks": [
      2,
      "never"
    ],
    "operator-linebreak": [
      2,
      "after"
    ],
    "operator-assignment": [
      2,
      "always"
    ],
    "one-var": [
      2,
      "never"
    ],
    "object-curly-spacing": [
      2,
      "never"
    ],
    "no-unneeded-ternary": 2,
    "no-underscore-dangle": 0,
    "no-trailing-spaces": 2,
    "no-ternary": 0,
    "no-spaced-func": 2,
    "no-new-object": 2,
    "no-nested-ternary": 2,
    "no-multiple-empty-lines": [
      2, {
        "max": 2
      }
    ],
    "no-mixed-spaces-and-tabs": 2,
    "no-lonely-if": 2,
    "no-inline-comments": 2,
    "no-continue": 2,
    "no-array-constructor": 2,
    "new-parens": 2,
    "new-cap": 2,
    "max-nested-callbacks": [
      2,
      2
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "key-spacing": [
      2, {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "jsx-quotes": [
      2,
      "prefer-single"
    ],
    "indent": [
      2,
      2
    ],
    "func-style": [
      2,
      "expression"
    ],
    "consistent-this": [
      2,
      "self"
    ],
    "computed-property-spacing": [
      2,
      "never"
    ],
    "comma-style": [
      2,
      "last"
    ],
    "comma-spacing": [
      2, {
        "before": false,
        "after": true
      }
    ],
    "camelcase": 2,
    "brace-style": 2,
    "block-spacing": [
      2,
      "always"
    ],
    "array-bracket-spacing": [
      2,
      "never"
    ],
    "require-yield": 2,
    "prefer-template": 2,
    "prefer-spread": 2,
    "prefer-const": 2,
    "no-this-before-super": 2,
    "no-dupe-class-members": 2,
    "no-const-assign": 2,
    "no-class-assign": 2,
    "no-arrow-condition": 2,
    "generator-star-spacing": [
      2, {
        "before": true,
        "after": false
      }
    ],
    "constructor-super": 2,
    "arrow-spacing": 2,
    "arrow-parens": 2,
    "arrow-body-style": [
      2,
      "as-needed"
    ],
    "no-bitwise": 0,
    "no-case-declarations": 0,
    "no-cond-assign": 2,
    "no-console": 2,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-delete-var": 2,
    "no-dupe-keys": 2,
    "no-dupe-args": 2,
    "no-duplicate-case": 2,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": 0,
    "no-extra-semi": 2,
    "no-fallthrough": 2,
    "no-func-assign": 2,
    "no-inner-declarations": [
      2,
      "functions"
    ],
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-negated-condition": 0,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-plusplus": 0,
    "no-process-env": 0,
    "no-process-exit": 0,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-restricted-modules": 0,
    "no-restricted-syntax": 0,
    "no-shadow": 0,
    "no-sparse-arrays": 2,
    "no-undef": 2,
    "no-undef-init": 0,
    "no-unreachable": 2,
    "no-var": 0,
    "no-warning-comments": [
      0, {
        "terms": [
          "todo",
          "fixme",
          "xxx"
        ],
        "location": "start"
      }
    ],
    "accessor-pairs": 0,
    "eol-last": 0,
    "func-names": 0,
    "id-length": 0,
    "init-declarations": 0,
    "lines-around-comment": 0,
    "max-depth": [
      0,
      4
    ],
    "max-len": [
      0,
      80,
      4
    ],
    "max-params": [
      0,
      3
    ],
    "max-statements": [
      0,
      10
    ],
    "newline-after-var": 0,
    "object-shorthand": 0,
    "prefer-arrow-callback": 0,
    "id-match": 0,
    "use-isnan": 2,
    "valid-typeof": 2,
    "vars-on-top": 0,
    "yoda": [
      0,
      "never"
    ]
  }
}